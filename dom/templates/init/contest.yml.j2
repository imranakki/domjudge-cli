contests:
  - name: "{{ name }}"
    shortname: "{{ shortname }}"
    start_time: "{{ start_time }}"
    duration: "{{ duration }}"
    penalty_time: {{ penalty_time }}
    allow_submit: {{ allow_submit }}

    problems:
      from: "problems.yaml"

    teams:
      from: "{{ teams }}"
      delimiter: "{{ delimiter }}"
      rows: "{{ rows }}"
      name: "${{ name_column }}"
      affiliation: "${{ affiliation_column }}"
      {% if country_column %}country: "${{ country_column }}"{% endif %}
